#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/@nrwl+tao@18.2.4/node_modules/@nrwl/tao/node_modules:/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/@nrwl+tao@18.2.4/node_modules/@nrwl/node_modules:/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/@nrwl+tao@18.2.4/node_modules:/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/@nrwl+tao@18.2.4/node_modules/@nrwl/tao/node_modules:/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/@nrwl+tao@18.2.4/node_modules/@nrwl/node_modules:/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/@nrwl+tao@18.2.4/node_modules:/home/yimla/PROJECTS/personal_projects/yimla/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
