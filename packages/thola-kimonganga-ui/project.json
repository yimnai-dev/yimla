{
    "root": "packages/thola-kimonganga-ui",
    "sourceRoot": "packages/thola-kimonganga-ui",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vite build",
          "cwd": "packages/thola-kimonganga-ui"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "vite dev",
          "cwd": "packages/thola-kimonganga-ui"
        }
      },
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npm run test:integration && npm run test:unit",
          "cwd": "packages/thola-kimonganga-ui"
        }
      }  
    }
  }