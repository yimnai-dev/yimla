{
    "root": "packages/thola-kimonganga-api",
    "sourceRoot": "packages/thola-kimonganga-api/src",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go build -o=../../dist/packages/thola-kimonganga-api/ ./src/main.go",
          "cwd": "packages/thola-kimonganga-api"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "exec air",
          "cwd": "packages/thola-kimonganga-api"
        }
      } 
    }
  }